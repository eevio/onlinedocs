{
    "build": {
      "content": [
        {
          "files": [
            "index.md",
            "articles/**/*.md",
            "articles/**/toc.md"
          ],
          "exclude": [
            "articles/**/README.md"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "images/*",
            "articles/**/images/**"
          ]
        }
      ],
      "dest": "static",
      "globalMetadataFiles": [],
      "fileMetadataFiles": [],
      "template": [
        "default",
        "templates/material"
      ],
      "postProcessors": [],
      "markdownEngineName": "markdig",
      "noLangKeyword": false,
      "keepFileLink": false,
      "cleanupCacheHistory": false,
      "disableGitFeatures": false
    },
    "pdf": {
      "content": [
        {
          "files": [
            "index.md",
            "articles/**/*.md",
            "articles/**/toc.md"
          ],
          "exclude": [
            "articles/**/README.md"
          ]
        }
      ],
      "resource": [
        {
          "files": [
            "images/*",
            "articles/**/images/**"
          ]
        }
      ],
      "dest": "pdf"
    }
  }